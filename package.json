{
  "name": "@onolog/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check": "npm run lint && npm run test",
    "dev": "DEBUG=@onolog/api:* nodemon index.js",
    "lint": "eslint . --report-unused-disable-directives",
    "migrate": "sequelize db:migrate",
