    "debug": "^4.0.1",
    "dotenv": "^7.0.0",
    "express-jwt": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "graphql-sequelize": "^9.0.1",
    "graphql-yoga": "^1.14.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.20",
    "mysql2": "^1.5.3",
    "nodemon": "^1.17.5",
    "sequelize": "^5.1.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
